{
  "buildCommand": "cd ../.. && pnpm --filter @ecommerce/shared build && pnpm --filter admin build",
  "outputDirectory": "dist",
  "devCommand": "cd ../.. && pnpm --filter admin dev",
  "installCommand": "cd ../.. && pnpm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
